{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p autobuild start:dev",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "compile": "nexe server.js -r 'public/**/*.html' --build",
    "nexe": "nexe -i server.js -o myproject.bin -r 0.10.3 --build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-svelte-global-styles": "^1.0.3",
    "rollup": "^2.17.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.9.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "express": "^4.17.1",
    "gsap": "^3.3.4",
    "sass": "^1.26.8",
    "sirv-cli": "^0.4.4",
    "svelte-awesome": "^2.3.0",
    "svelte-preprocess": "^3.9.6"
  }
}
